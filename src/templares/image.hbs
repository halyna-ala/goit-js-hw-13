{{#each this}}

<li class="image-card">
    <div class="photo-card">
        <img class="image-item" src="{{webformatURL}}" alt="{{tags}}" data-source="{{largeImageURL}}" />
        <div class="stats">
            <p class="stats-item">
                <i class="material-icons">Likes</i>
                {{likes}}
            </p>
            <p class="stats-item">
                <i class="material-icons">Views</i>
                {{views}}
            </p>
            <p class="stats-item">
                <i class="material-icons">Comments</i>
                {{comments}}
            </p>
            <p class="stats-item">
                <i class="material-icons">Downloads</i>
                {{downloads}}
            </p>
        </div>
    </div>
</li>
{{/each}}